<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form">
    <!-- InformaciÃ³n del Transportitas -->
    <IconTabFilter icon="sap-icon://detail-view" key="typeGD" >
        <Page title="{i18n>titleTypeGD}:" >
            <FlexBox
                class="ne-flexbox2"
                alignItems="Center"
                alignContent="Center">
                <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop" header="{i18n>consignmentMaterials}"
                    press="getMaterials" frameType= "OneByOne">
                    <TileContent >
                        <ImageContent src="sap-icon://product" />
                    </TileContent>
                </GenericTile>
                <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop" header="{i18n>freelyUsableMaterials}"
                    press="getMaterials" frameType= "OneByOne">
                    <TileContent >
                        <ImageContent src="sap-icon://supplier" />
                    </TileContent>
                </GenericTile>
            </FlexBox>  
        </Page>
    </IconTabFilter>
</core:FragmentDefinition>